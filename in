def {add-mul} (\ {x y} {+ x (* x y)})
add-mul 10 20
add-mul 10
def {add-mul-ten} (add-mul 10)
add-mul-ten 50
def {fun} (\ {args body} {def (head args) (\ (tail args) body)})
fun {add-together x y} {+ x y}
add-together 10 20
fun {unpack f xs} {eval (cons f xs)}
fun {pack f & xs} {f xs}
def {uncurry} pack
def {curry} unpack
curry + {5 6 7}
uncurry head 5 6 7
def {add-uncurried} +
def {add-curried} (curry +)
add-curried {5 6 7}
add-uncurried 5 6 7
fun {head_ a} {eval (head a)}
head_ {1 2 3}